journal = {Journal of the Mechanics and Physics of Solids},
keywords = {Elastic Softening,Elastic stiffening,Geometric nonlinearities,Nonlinear metamaterials,Periodic lattices,Tensegrity prisms},
title = {{On the mechanical modeling of the extreme softening/stiffening response of axially loaded tensegrity prisms}},
