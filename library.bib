file = {:C$\backslash$:/Users/alexg/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Scherer et al. - 2017 - Simulation of Reacting Moving Granular Material in Furnaces and Boilers An Overview on the Capabilities of the D.pdf:pdf},
issn = {18766102},
