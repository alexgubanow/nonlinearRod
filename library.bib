doi = {10.4081/jae.2015.436},
file = {:C$\backslash$:/Users/alexg/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Ayuga - 2015 - Present and future of the numerical methods in buildings and infrastructures areas of biosystems engineering.pdf:pdf},
